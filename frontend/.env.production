# Mini-UPS Frontend Production Environment

# ========================================
# API Configuration
# ========================================
VITE_API_BASE_URL=https://api.miniups.com
VITE_WS_BASE_URL=wss://api.miniups.com

# ========================================
# App Configuration
# ========================================
VITE_APP_NAME=Mini-UPS
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Mini-UPS Logistics Management System

# ========================================
# Feature Flags (Production)
# ========================================
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true

# ========================================
# External Services (Production)
# ========================================
# Analytics
VITE_ANALYTICS_ID=GA_TRACKING_ID
VITE_SENTRY_DSN=https://your-sentry-dsn

# ========================================
# Security & Auth (Production)
# ========================================
VITE_SESSION_TIMEOUT=3600000
VITE_ENABLE_HTTPS=true

# ========================================
# Performance & Debugging (Production)
# ========================================
VITE_LOG_LEVEL=error
VITE_ENABLE_SOURCE_MAPS=false
VITE_ENABLE_BUNDLE_ANALYZER=false

# ========================================
# CDN & Assets (Production)
# ========================================
VITE_CDN_URL=https://cdn.miniups.com
VITE_STATIC_ASSETS_URL=https://assets.miniups.com